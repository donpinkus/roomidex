<% end %>
<section class="container textCenter">
	<%= image_tag "roomidexlogobanner-beta.png", class: "roomidex-logo" %>
	<div class="row movingInput">
		<div class="halo clearfix col-md-offset-3 col-md-6">
			<div class="row">
				<div class="col-md-7">
					<select class="form-control">
						<option>Where are you moving?</option>
					  <option>San Francisco, CA</option>
					  <option>New York, NY</option>
					</select>
				</div>
				<div class="col-md-4">
					<a href="#" class="fbLogin">
						<%= image_tag "facebook-login.png" %>
					</a>
				</div>
			</div>
		</div>
	</div>
</section>